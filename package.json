    "register-service-worker": "^1.7.1",
    "vue": "^2.6.14",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.1.6",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.5.13",
    "vuex": "^3.6.2",
    "vuex-composition-helpers": "^1.0.23"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^0.15.0",
